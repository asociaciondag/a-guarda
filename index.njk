        <h2>{{ programa.titulo }}</h2>
        <ul>
          {% for evento in programa.info %}
          <li>
            <time>{{ evento.hora }}</time>
            <div>
              {{ evento.info | md | safe }}
            </div>
          </li>
          {% endfor %}
        </ul>

        <h2>{{ horarios.titulo }}</h2>
        <ul>
          {% for evento in horarios.info %}
          <li>
            <time>{{ evento.hora }}</time>
            <div>
              {{ evento.info | md | safe }}
            </div>
          </li>
          {% endfor %}
        </ul>

        <div>
          {{ info | md | safe }}
        </div>
      </article>

      <aside class="project-news-external wrapper">
        <ul>
          {% for medio in medios %}
          <li>
              <h3>
                <a href="{{ medio.url }}">
                  {{ medio.title }}
                </a>
              </h3>
              <small>{{ medio.source }}</small>
            </a>
          </li>
          {% endfor %}
        </ul>
      </aside>
